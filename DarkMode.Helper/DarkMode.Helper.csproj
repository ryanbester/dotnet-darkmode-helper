<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0-windows7.0</TargetFramework>
    <Authors>Ryan Bester</Authors>
    <PackageTags>darkmode, dotnet, dark, helper</PackageTags>
    <PackageVersion>1.0.1</PackageVersion>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/ryanbester/dotnet-darkmode-helper</PackageProjectUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/ryanbester/dotnet-helper</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
	<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DarkMode" Version="1.1.3" />
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="../README.md" Pack="true" PackagePath="\" />
  </ItemGroup>

</Project>
